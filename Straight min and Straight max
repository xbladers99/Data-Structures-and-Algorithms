#include <bits/stdc++.h>
using namespace std;


int main()
{
    
    int a[]={1,6,5,7,9,4,3,56,14,20};
    int max=a[0],min=a[0];
    for(int i;i<10;i++)
    {
        if(a[i]>max)
        max=a[i];
        if(a[i]<min)
        min=a[i];
    }
    
    cout<<"maximum element is"<<max<<endl;
    cout<<"minimmum element is"<<min<<endl;
}
